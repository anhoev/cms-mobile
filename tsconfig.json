{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "inlineSourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true
    },
    "files": [
        "app/app.component.ts",
        "app/main.ts",
        "app/compat.d.ts",
        "app/ns-location-strategy.ts",
        "app/shared/config.ts",
        "app/shared/utils/action-bar-util.ts",
        "app/shared/view-models/user-view-model.ts",
        "app/views/list/list.ts",
        "app/views/list/grocery-list.ts",
        "app/views/login/login.ts",
        "app/views/main-page/main-page.ts",
        "app/views/register/register.ts",
        "node_modules/tns-core-modules/tns-core-modules.d.ts"
    ],
    "filesGlob": [
        "node_modules/tns-core-modules/tns-core-modules.d.ts",
        "app/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "platforms"
    ]
}